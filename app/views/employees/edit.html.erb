<% render "form" %>
<%= form_for @employee do |e| %>
<%= e.label :last_name %>
<%= e.text_field :last_name %>

<%= e.label :first_name %>
<%= e.text_field :first_name %>

<%= e.label :alias %>
<%= e.text_field :alias %>

<%= e.label :title %>
<%= e.text_field :title %>

<%= e.label :office %>
<%= e.text_field :office %>


<%= e.label :img_url %>
<%= e.text_field :img_url %>
<%= e.label :dog %>
<%= e.collection_select :dog_id, @dogs, :id, :name %>
<%= e.submit "Update Employees" %>
<% end %>
